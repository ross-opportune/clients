<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Time_Completed_Less_Time_Started</fullName>
    <active>true</active>
    <errorConditionFormula>Time_Started__c &gt;= Time_Completed__c</errorConditionFormula>
    <errorMessage>Time Completed must be later than Time Started</errorMessage>
</ValidationRule>
