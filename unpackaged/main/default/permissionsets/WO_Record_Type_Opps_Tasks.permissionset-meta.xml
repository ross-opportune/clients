<?xml version="1.0" encoding="UTF-8"?>
<PermissionSet xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>Grants access to create WOs with a record type of Downtime, Non-Downtime, or Artificial Lift.</description>
    <hasActivationRequired>false</hasActivationRequired>
    <label>WO Record Type - Opps Tasks</label>
    <recordTypeVisibilities>
        <recordType>WorkOrder.Artificial_Lift</recordType>
        <visible>true</visible>
    </recordTypeVisibilities>
    <recordTypeVisibilities>
        <recordType>WorkOrder.Downtime</recordType>
        <visible>true</visible>
    </recordTypeVisibilities>
    <recordTypeVisibilities>
        <recordType>WorkOrder.Non_Downtime</recordType>
        <visible>true</visible>
    </recordTypeVisibilities>
</PermissionSet>
